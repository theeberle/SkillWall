<div class="skill-form">
<h1>Add your skill!</h1>
<%= simple_form_for(@skill) do |f| %>
  <%= f.input :name, label: "Skill" %>
  <%= f.input :category %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :address %>
  <%= f.button :submit, class: "btn-turquoise" %>
<% end %>
</div>
